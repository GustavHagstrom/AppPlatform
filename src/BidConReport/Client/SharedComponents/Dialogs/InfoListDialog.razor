<MudDialog>
    <DialogContent>
        @foreach (var infostring in Info)
        {
            <MudText Typo="Typo.h6">@infostring</MudText>
        }
    </DialogContent>
    <DialogActions>
        <MudButton Color="Color.Success" OnClick="Submit">Ok</MudButton>
    </DialogActions>
</MudDialog>
@code {
    public const string InfoParameter = "Info";
    [CascadingParameter] public required MudDialogInstance MudDialog { get; set; }
    [Parameter] public required HashSet<string> Info { get; set; } = new();

    void Submit() => MudDialog.Close(DialogResult.Ok(true));
}